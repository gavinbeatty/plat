#ifndef PLAT_CONFIG_H_
#define PLAT_CONFIG_H_


#define PLAT_VERSION_MAJOR      @VERSION_MAJOR@
#define PLAT_VERSION_MINOR      @VERSION_MINOR@
#define PLAT_VERSION_PATCH      @VERSION_PATCH@
#define PLAT_VERSION_STR        "@VERSION@"
#define PLAT_HOMEPAGE_STR       "@HOMEPAGE@"
#define PLAT_CONTACT_STR        "@CONTACT@"

#define PLAT_VERSION \
    (((PLAT_VERSION_MAJOR)<<16)|((PLAT_VERSION_MINOR)<<8)|((PLAT_VERSION_PATCH)+0))
#define PLAT_VERSION_CHECK(major,minor,patch) \
    (((major) << 16)|((minor) << 8)|((patch)+0))

#endif
